{
    "name": "madridianfox/laravel-metrics",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Madridianfox\\LaravelMetrics\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Madridianfox\\LaravelMetrics\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Madridianfox\\LaravelMetrics\\MetricsServiceProvider"
            ]
        }
    },
    "require": {
        "php": ">=8.1",
        "psr/http-message": "^1.0",
        "guzzlehttp/guzzle": "^7.0",
        "madridianfox/laravel-prometheus": "v0.1.0"
    },
    "require-dev": {
        "franzl/studio": "^0.16.0",
        "orchestra/testbench": "^7.0"
    },
    "config": {
        "allow-plugins": {
            "franzl/studio": true
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "/home/madridianfox/work/homework/composer-packages/laravel-prometheus",
            "options": {
                "versions": {
                    "madridianfox/laravel-prometheus": "v0.1.0"
                },
                "symlink": false
            }
        }
    ]
}
